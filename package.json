{
  "name": "prebid-outstream",
  "version": "0.1.0-pre",
  "description": "Outstream renderer for Prebid.",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config build-utils/webpack.config.js --env.env=dev",
    "build": "webpack --config build-utils/webpack.config.js --env.env=prod",
    "build-analyze": "npm run build -- --env.addon=bundleanalyze --env.addon=bundlevisualizer",
    "test": "jest",
    "test-watch": "jest --watch",
    "coverage": "jest --coverage",
    "lint": "prettier --check \"{src,test,__mocks__}/**/*.ts*\"",
    "lint-fix": "npm run lint -- --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prebid/prebid-outstream.git"
  },
  "author": "Agam Mathur",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@types/jest": "^26.0.20",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.0.0",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^7.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^29.7.0",
    "prettier": "^2.2.1",
    "style-loader": "^1.2.1",
    "ts-jest": "^29.1.5",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.2",
    "url-loader": "^4.1.0",
    "webpack": "^5.92.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-conditional-loader": "^1.0.12",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^5.0.9",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "fluid-player": "^3.27.0"
  }
}
