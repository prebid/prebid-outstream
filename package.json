    "@types/jsdom": "^16.2.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.0.0",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^7.2.0",
    "eslint-loader": "^4.0.2",
